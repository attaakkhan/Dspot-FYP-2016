<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:id="@+id/dsmvlksdmvlksmvkl">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Name     "
        android:id="@+id/textView111"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#ffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="IP:PORT   "
        android:id="@+id/textView121"
        android:layout_below="@+id/textView111"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#ffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:id="@+id/list_createbellip_ipport"
        android:autoText="true"
        android:textColor="#ffffff"
        android:layout_below="@+id/textView111"
        android:layout_alignBottom="@+id/textView121"
        android:layout_toRightOf="@+id/textView121"
        android:layout_alignRight="@+id/list_mac_name"
        android:layout_alignEnd="@+id/list_mac_name" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="sdmsldmv"
        android:id="@+id/list_createbellip_name"
        android:textColor="#ffffff"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/create_b_ip_cb"
        android:layout_toRightOf="@+id/textView121"
        android:layout_toEndOf="@+id/textView121" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/create_b_ip_cb"
        android:checked="false"
        android:text="Select"
        android:textColor="#ffffff"
        android:background="#898080"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

</RelativeLayout>